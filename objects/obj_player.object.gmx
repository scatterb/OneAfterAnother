            <kind>1</kind>
            <string>/// Move the player in the step event  

var up = keyboard_check(ord('W'));
var down = keyboard_check(ord('S'));
var left = keyboard_check(ord('A'));
var right = keyboard_check(ord('D'));


// Move up
if(up){
    phy_position_y -= spd;
    if (hasshot == false) {
        sprite_index = spr_player_up;
    }
    image_speed = .4;
}
// Move down
if(down) {
    phy_position_y += spd;
    if (hasshot == false) {
        sprite_index = spr_player_down;
    }
    image_speed = .4;
}

// Move left
if(left) {
    phy_position_x -= spd;
    if (hasshot == false) {
        sprite_index = spr_player_left;
    }
    image_speed = .4;
}

// Move right
if(right) {
    phy_position_x += spd;
    if (hasshot == false) {
        sprite_index = spr_player_right;
    }
    image_speed = .4;
}

//stop animating
if(!down and !right and !left and !up) {
    image_speed = 0;
}
</string>
          </argument>
