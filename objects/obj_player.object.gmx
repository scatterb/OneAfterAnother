        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Weapon usage scripts

// Shooting keys

shootup = keyboard_check(ord('I'));
shootdown = keyboard_check(ord('K'));
shootleft = keyboard_check(ord('J'));
shootright = keyboard_check(ord('L'));

// When player shoots
if(shootup or shootdown or shootleft or shootright) {
    if(canshoot == true) {
            for(i=0;i&lt;=global.pellets;i++) {
                    with (instance_create(x, y, weapon)) {
                        player = obj_player;
                        speed = player.weapon.speed;
        
                        if(player.shootup) {
                            direction = 0;
                            player.sprite_index = spr_player_up;
                        }
                        if(player.shootright) {
                            direction = 90;
                            player.sprite_index = spr_player_right;
                        }
                        if(player.shootdown) {
                            direction = 180;
                            player.sprite_index = spr_player_down;
                        }
                        if(player.shootleft) {
                            direction = 270;
                            player.sprite_index = spr_player_left;
                        }   
                    }
                }
            
        // Set cooldown on weapon so button can be held
            canshoot = false;
            wpn_cd = weapon.wpn_rof;
        // Set cooldown on movement. if cd is on, model doesn't turn from movement
            hasshot = true;
            movesprite = 60;
            
    }
}

// Cooldown on weapon

if (wpn_cd &lt;= 0) {
    canshoot = true;
} else {
    wpn_cd -= 1;
}

// Model turn cooldown

if (movesprite &lt;= 0) {
    hasshot = false;
} else {
    movesprite -= 1;
}
</string>
